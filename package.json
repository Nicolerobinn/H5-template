{
  "name": "h5_template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prettier-format": "prettier --config .prettierrc \"src/**/*.js\"   \"src/**/*.ts\"  \"src/**/*.tsx\"  --write",
    "dev": "yarn start",
    "lint": "yarn lint:es && yarn lint:style",
    "lint:es": "eslint . --fix",
    "lint:style": "stylelint --fix \"src/**/*.scss\"",
    "build": "webpack --config ./config/webpack/webpack.config.js",
    "build:rc": "cross-env NODE_ENV=rc API_ENV=rc webpack --config ./config/webpack/webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production API_ENV=production webpack --config ./config/webpack/webpack.config.js",
    "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack/webpack.config.js",
    "analyzer": "yarn build  --analyze",
    "analyzer:prod": "yarn build:prod  --analyze",
    "prepare": "node node_modules/husky-ci"
  },
  "author": "cweb130@163.com",
  "engines": {
    "node": ">=14.18.1"
  },
  "dependencies": {
